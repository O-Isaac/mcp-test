#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/build/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/build/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/cli/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/inspector/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules/@modelcontextprotocol/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/@modelcontextprotocol+inspector@0.10.2_@types+node@22.14.1_typescript@5.8.3/node_modules:/home/usumaniana/dev/mcp-aa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/build/cli.js" "$@"
else
  exec node  "$basedir/../../cli/build/cli.js" "$@"
fi
